#!/usr/bin/env python3
"""
Wifitex command-line interface entry point
"""

import sys
import os

# Add the current directory to Python path to ensure we use local development version
current_dir = os.path.dirname(os.path.abspath(__file__))
project_root = os.path.dirname(current_dir)
if project_root not in sys.path:
    sys.path.insert(0, project_root)

if __name__ == '__main__':
    from wifitex.__main__ import entry_point
    entry_point()
